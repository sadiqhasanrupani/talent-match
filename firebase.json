{
"emulators": {
    "dataconnect": {
    "dataDir": "dataconnect/.dataconnect/pgliteData"
    }
},
"dataconnect": {
    "source": "dataconnect"
},
"storage": {
    "rules": "storage.rules"
},
"hosting": {
    "public": "public",
    "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
    ],
    "rewrites": [
    {
        "source": "**",
        "destination": "/index.html"
    }
    ]
}
}
